_restaurant = [];\n                } else {\n                    three_restaurant.push(new_restaurant);\n                }\n            }\n        }\n\n        onMounted(makeDataRestaurant);\n\n        // return\n        return {\n            data_restaurant\n        }\n    },\n}\n</script>\n\n<style lang=\"scss\">\n    .home {\n        .header {\n            height: 120px;\n            width: 100%;\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            img {\n                width: 300px;\n            }\n        }\n    }\n</style>"]},"metadata":{},"sourceType":"module"}